n1 = a MOD 10
n2 = a MOD 2
n3 = a/100
if (n1 + n2 > n3)
	print “inside 1st if”
else if (n1 + n2 + n3 > n3 + 3)
	print “inside 2nd if”
else if ((n1 + n2)/n3 EQUALS 0)
	print “inside 3rd if”
else
	print “Last if”
inputs
1.	a = 987
2.	a = 341
3.	a = 247
